<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd" name="rest-with-spring-boot-and-kotlin-erudio"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet encoding="UTF-8">
      <directory>src/main/resources</directory>
      <includes>
        <include>**/*.yml</include>
        <include>**/*.sql</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8" filtered="true" packaged="true">
      <directory>src/main/kotlin</directory>
      <includes>
        <include>**/*.kt</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/test/resources</directory>
      <includes>
        <include>**/*.yml</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8" filtered="true" packaged="true">
      <directory>src/test/kotlin</directory>
      <includes>
        <include>**/*.kt</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
